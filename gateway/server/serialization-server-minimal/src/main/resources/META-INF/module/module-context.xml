<?xml version="1.0" encoding="UTF-8"?>

<beans
    xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:c="http://www.springframework.org/schema/c"
    xsi:schemaLocation="
       http://www.springframework.org/schema/beans 
       http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

    <bean
        id="vnet.sms.gateway.server.serialization-server.serializationServerDescription"
        class="vnet.sms.gateway.server.framework.spi.GatewayServerDescription"
        c:name="Serialization Protocol Gateway Server"
        c:majorVersion="${parsedVersion.majorVersion}"
        c:minorVersion="${parsedVersion.minorVersion}"
        c:incrementalVersion="${parsedVersion.incrementalVersion}"
        c:qualifier="${parsedVersion.qualifier}"
        c:buildNumber="${buildNumber}" />

</beans>